Description :   WAP to implement your own islower() function
Input       :   Enter the character: a
Output      :   Entered character is lower case alphabet
*/

#include<stdio.h>
int is_lower(int c)
{
    if( c>=97 && c<=122 )
    {
        return 1;
    }
    else
    {
        return 0;
    }

}

int main()
{
    char ch;
    int ret;
    printf("Enter the charachter: ");
    scanf("%c",&ch);
    ret = is_lower(ch);
    if(ret==1)
    {
        printf("Entered character is lower case alphabet \n");
    }
    else
    {
        printf("Entered character is not lower case alphabet \n");

    }

}
